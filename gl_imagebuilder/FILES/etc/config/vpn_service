config general 'global'
	option enable '0'
	option access 'DROP'

config service 'vpn'
	option auth 'SHA1'
	option proto 'udp'
	option port '1194'
	option dev 'tun-SERVER'
	option dev_type 'tun'
	option cipher 'BF-CBC'
	option comp 'adaptive'
	option subnet '10.8.0.0'
	option mask '255.255.255.0'
	option host '127.0.0.1'
